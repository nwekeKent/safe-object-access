{
  "name": "safe-object-access",
  "version": "1.0.1",
  "description": "A robust, type-safe utility for safely accessing deeply nested object properties using string paths with full TypeScript autocomplete support.",
  "keywords": [
    "typescript",
    "safe-access",
    "get",
    "nested-objects",
    "type-safe",
    "path-access"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwekeKent/safe-object-access.git"
  },
  "bugs": {
    "url": "https://github.com/nwekeKent/safe-object-access/issues"
  },
  "homepage": "https://github.com/nwekeKent/safe-object-access#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^25.0.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
